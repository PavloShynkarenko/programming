#include <stdio.h> // підключаємо бібліотеку

int main()
{
   int n;
   int count = 0;
   // оголошуємо змінні

   printf("Введіть натуральне число n: "); // виводимо на екран текст, в якому просимо ввести натуральне число
   scanf("%d", &n); // скануємо дані

   for (int i = 1; i < n; i++)
   {

      if (n % i == n / i)
      {
         count++;
      }
   } // створємо цикл в якому знаходимо кількість рівних дільників числа

   printf("Кількість рівних дільників числа %d: %d\n", n, count); // виводимо результат

   return 0; //повертаємо значення 0
}
